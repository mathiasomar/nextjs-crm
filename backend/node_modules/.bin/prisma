#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules/prisma/build/node_modules:/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules/prisma/node_modules:/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules:/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules/prisma/build/node_modules:/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules/prisma/node_modules:/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules:/mnt/c/Users/ismaaa/Documents/ommy projects/nextjs-crm/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules/prisma/build/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/prisma@7.0.0_@types+react@1_f38d292ac65e7c16722e9b563819994b/node_modules/prisma/build/index.js" "$@"
fi
