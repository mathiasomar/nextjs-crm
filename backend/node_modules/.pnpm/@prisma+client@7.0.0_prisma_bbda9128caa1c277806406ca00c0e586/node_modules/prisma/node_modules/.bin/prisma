#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_0450c16527ac337316316b1195371b92/node_modules/prisma/build/node_modules:/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_0450c16527ac337316316b1195371b92/node_modules/prisma/node_modules:/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_0450c16527ac337316316b1195371b92/node_modules:/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_0450c16527ac337316316b1195371b92/node_modules/prisma/build/node_modules:/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_0450c16527ac337316316b1195371b92/node_modules/prisma/node_modules:/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/prisma@7.0.0_@types+react@1_0450c16527ac337316316b1195371b92/node_modules:/mnt/c/Users/Admin/Documents/ommy/projects/CRM/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/index.js" "$@"
else
  exec node  "$basedir/../../build/index.js" "$@"
fi
